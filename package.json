{
    "name": "crud",
    "version": "1.0.0",
    "description": "A simple CRUD application using Typescript, GraphQL, Apollo-server and PostgreSQL",
    "main": "index.js",
    "scripts": {
        "watch": "tsc -w",
        "start": "node dist/index.js",
        "dev": "nodemon dist/index.js",
        "start-ts": "ts-node src/index.ts",
        "dev-ts": "nodemon --exec ts-node src/index.ts",
        "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tusharsb-12/CRUD.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/tusharsb-12/CRUD/issues"
    },
    "homepage": "https://github.com/tusharsb-12/CRUD#readme",
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/node": "^16.10.5",
        "nodemon": "^2.0.13",
        "ts-node": "^10.3.0",
        "typescript": "^4.4.4"
    },
    "dependencies": {
        "apollo-server-express": "^3.4.0",
        "express": "^4.17.1",
        "graphql": "^15.6.1",
        "pg": "^8.7.1",
        "reflect-metadata": "^0.1.13",
        "type-graphql": "^1.1.1",
        "typeorm": "^0.2.38"
    }
